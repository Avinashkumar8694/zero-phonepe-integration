{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "./dist",
      "noUnusedParameters": false,
      "strictNullChecks": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  